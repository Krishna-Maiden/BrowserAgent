@using WebUI.Shared

<Router AppAssembly="@typeof(Program).Assembly">
    <Found>
        <RouteView DefaultLayout="@typeof(MainLayout)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(MainLayout)">
            <p>Sorry, nothing found at this address.</p>
        </LayoutView>
    </NotFound>
</Router>
